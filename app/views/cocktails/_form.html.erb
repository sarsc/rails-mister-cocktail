
<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :description, as: :text%>
  </div>

  <%= f.submit "create a cocktail", class: "btn btn-submit" %>
<% end %>
